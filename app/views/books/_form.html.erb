<%= simple_form_for(book) do |f| %>
  <%= f.input :book_name, :label => "Book Name:" %>
  <%= f.input :author, :label => "Author:" %>
  <%= f.input :published_at, :label => "Published at:" %>
  <%= f.input :isbn_number, :label => "ISBN Number:" %>
  <%= f.input :category, :label => "Category:" %>
  <%= f.submit "Submit" %>
<% end %>
